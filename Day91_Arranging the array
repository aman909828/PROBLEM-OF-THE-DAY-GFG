
from typing import List


class Solution:
    def Rearrange(self, n : int, arr : List[int]) -> None:
        a=[]
        b=[]
        for i in range(n):
            if arr[i]<0:
                a.append(arr[i])
            else:
                b.append(arr[i])
        arr.clear()
        for i in a:
            arr.append(i)
        for i in b:
            arr.append(i)
        return arr

            
        

