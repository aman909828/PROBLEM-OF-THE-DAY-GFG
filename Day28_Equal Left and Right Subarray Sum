# User function Template for python3

class Solution:
    # Complete this function
    
    #Function to find equilibrium point in the array.
    def equalSum(self,A, N):
        sums=sum(A)
        x=0
        for i in range(N):
            sums-=A[i]
            if sums==x:
                return i+1
            x+=A[i]
        return -1
        # Your code here

