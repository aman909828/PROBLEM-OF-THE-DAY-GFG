#User function Template for python3

class Solution:
    def transpose(self, matrix, n):
        arr=[[0 for i in range(n)]for j in range(n)]
        cnt=0
        for i in range(n):
            for j in range(n):
               # print(matrix[i][j])
                arr[j][cnt]=matrix[i][j]
            cnt+=1
        for i in range(n):
            for j in range(n):
                matrix[i][j]=arr[i][j]
        # Write Your code here
